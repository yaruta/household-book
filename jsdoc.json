{
    "source": {
      "include": ["src"],
      "includePattern": ".*\\.(js|jsx)$",
      "excludePattern": "(node_modules|dist)"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "readme": "README.md"
    },
    "plugins": ["plugins/markdown"],
    "tags": {
      "allowUnknownTags": true
    },
    "templates": {
      "default": {
        "includeDate": false
      }
    }
}